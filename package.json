{
  "name": "mybucket",
  "version": "1.0.0",
  "description": "MERN ecommerce",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/db/seeders/seeder",
    "data:destroy": "node backend/db/seeders/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamprabase/My-Bucket.git"
  },
  "keywords": [
    "mern",
    "shooping",
    "cart",
    "react",
    "nodejs"
  ],
  "author": "Prabesh Bikram Shahi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iamprabase/My-Bucket/issues"
  },
  "homepage": "https://github.com/iamprabase/My-Bucket#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.10.9",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
